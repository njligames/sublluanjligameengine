local BaseClass = require "NJLI.STATEMACHINE.NodeEntityState"

local ${1:NodeEntityState} = {}
${1:NodeEntityState}.__index = ${1:NodeEntityState}

--#############################################################################
--DO NOT EDIT ABOVE
--#############################################################################

