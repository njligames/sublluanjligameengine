local __ctor = function(self, init)
  --TODO: construct this Entity
end

local __dtor = function(self)
  --TODO: destruct this Entity
end

local __load = function(self)
  --TODO: load this Entity
end

local __unLoad = function(self)
  --TODO: unload this Entity
end

--############################################################################# 

function ${1:NodeEntityState}:enter()
	BaseClass.enter(self)
    print("${1:NodeEntityState}:enter()")
end

function ${1:NodeEntityState}:update(timeStep)
	BaseClass.update(self, timeStep)
    print("${1:NodeEntityState}:update(timeStep)")
end

function ${1:NodeEntityState}:exit()
	BaseClass.exit(self)
    print("${1:NodeEntityState}:exit()")
end

function ${1:NodeEntityState}:onMessage()
	BaseClass.onMessage(self)
    print("${1:NodeEntityState}:onMessage()")
end

function ${1:NodeEntityState}:touchDown(rayContact)
	BaseClass.touchDown(self, rayContact)
    print("${1:NodeEntityState}:touchDown(rayContact)")
end

function ${1:NodeEntityState}:touchUp(rayContact)
	BaseClass.touchUp(self, rayContact)
    print("${1:NodeEntityState}:touchUp(rayContact)")
end

function ${1:NodeEntityState}:touchMove(rayContact)
	BaseClass.touchMove(self, rayContact)
    print("${1:NodeEntityState}:touchMove(rayContact)")
end

function ${1:NodeEntityState}:touchCancelled(rayContact)
	BaseClass.touchCancelled(self, rayContact)
    print("${1:NodeEntityState}:touchCancelled(rayContact)")
end

function ${1:NodeEntityState}:collide(otherNode, collisionPoint)
	BaseClass.collide(self, collisionPoint)
    print("${1:NodeEntityState}:collide(otherNode, collisionPoint)")
end

function ${1:NodeEntityState}:near(otherNode)
	BaseClass.near(self, otherNode)
    print("${1:NodeEntityState}:near(otherNode)")
end

function ${1:NodeEntityState}:actionUpdate(action, timeStep)
	BaseClass.actionUpdate(self, timeStep)
    print("${1:NodeEntityState}:actionUpdate(action, timeStep)")
end

function ${1:NodeEntityState}:actionComplete(action)
	BaseClass.actionComplete(self, action)
    print("${1:NodeEntityState}:actionComplete(action)")
end

--#############################################################################
--End Custom Code
