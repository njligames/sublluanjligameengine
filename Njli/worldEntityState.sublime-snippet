setmetatable(${1:worldEntityState}, {
    __index = BaseClass,
    __call = function (cls, ...)
      local self = setmetatable({}, cls)
      --Create the base first
      BaseClass._create(self, ...)
      self:_create(...)
      return self
    end,
  })

function ${1:worldEntityState}:className()
  return "${1:worldEntityState}"
end

function ${1:worldEntityState}:class()
  return self
end

function ${1:worldEntityState}:superClass()
  return BaseClass
end

function ${1:worldEntityState}:__gc()
  --Destroy derived class first
  ${1:worldEntityState}._destroy(self)
  --Destroy base class after derived class
  BaseClass._destroy(self)
end

function ${1:worldEntityState}:__tostring()
  local ret = self:className() .. " =\n{\n"

  for pos,val in pairs(self) do
    ret = ret .. "\t" .. "["..pos.."]" .. " => " .. type(val) .. " = " .. tostring(val) .. "\n"
  end

  ret = ret .. "\n\t" .. tostring_r(BaseClass) .. "\n}"
  return ret .. "\n\t" .. tostring_r(getmetatable(self)) .. "\n}"
end

function ${1:worldEntityState}:_destroy()
  assert(not self.__${1:worldEntityState}CalledLoad, "Must unload before you destroy")
  __dtor(self)
end

function ${1:worldEntityState}:_create(init)
  self.__${1:worldEntityState}CalledLoad = false
  __ctor(self, init)
end

function ${1:worldEntityState}:load()
  --load base first
  BaseClass.load(self)

  --load derived last...
  __load(self)

  self.__${1:worldEntityState}CalledLoad = true
end

function ${1:worldEntityState}:unLoad()
  assert(self.__${1:worldEntityState}CalledLoad, "Must load before unloading")

  --unload derived first...
  __unLoad(self)
  self.__${1:worldEntityState}CalledLoad = false

  --unload base last...
  BaseClass.unLoad(self)
end

return ${1:worldEntityState}

